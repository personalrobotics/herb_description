        <inertial>
            <origin xyz="0.00498512 -0.00022942 0.13271662"/>
            <mass value="2.40016804"/>
            <inertia ixx="0.01491672" ixy="0.00001741" ixz="-0.00150604"
                                      iyy="0.01482922" iyz="-0.00002109"
