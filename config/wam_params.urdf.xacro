            <mass value="9.97059584"/>
            <inertia ixx="0.10916849" ixy="0.00640270" ixz="0.02557874"
                                      iyy="0.18294303" iyz="0.00161433"
                                                       izz="0.11760385"/>
        </inertial>
    </link>

    <link name="wam1">
